pipeline {
    agent any

    stages {
        stage('SCM ') {
            steps {
                echo 'geting from github'
                echo 'SCM Compleed'
            }
        }
        stage('Build ') {
            steps {
                sh 'date'
                echo 'Build Compleed'
            }
        }
        stage('Test') {
            steps {
                echo 'hello'
                sh 'date'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy Compleed'
            }
        }
        
    }
}
